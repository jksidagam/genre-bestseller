{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/genre-bestseller",
	"version": "0.1.0",
	"title": "Genre Bestseller",
	"category": "widgets",
	"icon": "book",
	"description": "A block displaying the bestselling book for the selected genre.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"genre": {
			"type": "string",
			"default": ""
		},
		"title": {
			"type": "string",
			"default": "Bestsellers"
		},
		"bookData": {
			"type": "object",
			"default": null
		},
		"amazon_link": {
			"type": "string",
			"default": "https://www.amazon.co.uk/"
		}
	},
	"textdomain": "genre-bestseller",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}